    { echo "configure: error: can't find ssl plugin install dir from $withval" 1>&2; exit 1; }
    SSL_LIBRARIES=""
  elif test \! -r $SSL_LIBRARIES ; then
    { echo "configure: error: can't access ssl plugin library $SSL_LIBRARIES" 1>&2; exit 1; }
    SSL_INCLUDE_DIR=""
    SSL_LIBRARIES=""
  elif test \! -r ${SSL_INCLUDE_DIR}/des.h ; then
    { echo "configure: error: can't access ssl plugin include directory $SSL_INCLUDE_DIR" 1>&2; exit 1; }
    SSL_INCLUDE_DIR=""
    SSL_LIBRARIES=""
  else
    echo "$ac_t""yes -- library $SSL_LIBRARIES, include dir $SSL_INCLUDE_DIR" 1>&6
  fi
fi
else
  echo "$ac_t""not given" 1>&6
fi



