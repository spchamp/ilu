#include <string.h>

int main (int ac, char **av)
{
  char buf[2048];

  if (ac < 2) {
    fprintf(stderr, "Usage:  %s PATH\n", av[0]);
    exit(1);
  };
  if (realpath(av[1], buf) != 0)
    printf("%s\n", buf);
  else {
    fprintf(stderr, "Error:  %s\n", strerror(errno));
    exit(1);
  }
  return 0;
}
