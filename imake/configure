        java_includebase=${ILUJAVA_S_JH}
     fi
  fi
  if test "$java_includebase" != "" ; then
     echo "$ac_t""${java_includebase}" 1>&6
  elif test "$java_version" = "1.1"; then
     if test -d /usr/java1.1/include ; then
        java_includebase="/usr/java1.1"
        if test -f /usr/java1.1/lib/classes.zip ; then
            java_coreclasses=/usr/java1.1/lib/classes.zip
        fi
        echo "$ac_t""${java_includebase}" 1>&6
     else
        unset java_version
        { echo "configure: error: Can't configure Java 1.1 (didn't find include directory)" 1>&2; exit 1; }
     fi
  elif test "$java_version" = "1.2"; then
     if test -d /usr/java1.2/include ; then
        java_includebase="/usr/java1.2"
        echo "$ac_t""${java_includebase}" 1>&6
     else
        unset java_version
        { echo "configure: error: Can't configure Java 1.2 (didn't find include directory)" 1>&2; exit 1; }
     fi
  else
     unset java_version
     { echo "configure: error: Can't configure unknown Java version" 1>&2; exit 1; }
  fi
