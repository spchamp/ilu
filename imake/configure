    { echo "configure: error: can't find zlib include dir from $withval" 1>&2; exit 1; }
    zlib_library=""
  elif test \! -r $zlib_library ; then
    { echo "configure: error: can't access zlib library $zlib_library" 1>&2; exit 1; }
    zlib_include_dir=""
    zlib_library=""
  elif test \! -r ${zlib_include_dir}/zlib.h ; then
    { echo "configure: error: can't access zlib include directory $zlib_include_dir" 1>&2; exit 1; }
    zlib_include_dir=""
    zlib_library=""
  else
    echo "$ac_t""yes -- library $zlib_library, include dir $zlib_include_dir" 1>&6
    ILU_ZLIB_LIBRARY="$zlib_library"
    ILU_ZLIB_INCLUDE_DIR="$zlib_include_dir"
  fi
fi
else
  echo "$ac_t""no, by default" 1>&6
fi




